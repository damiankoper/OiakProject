{
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "files.associations": {
        "*.view": "html",
        "*.blade.*": "blade",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "array": "cpp",
        "chrono": "cpp",
        "ctime": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "fstream": "cpp",
        "functional": "cpp",
        "optional": "cpp",
        "ratio": "cpp",
        "sstream": "cpp",
        "tuple": "cpp",
        "utility": "cpp",
        "cinttypes": "cpp",
        "iomanip": "cpp",
        "deque": "cpp",
        "numeric": "cpp",
        "bitset": "cpp",
        "cstddef": "cpp",
        "memory": "cpp",
        "variant": "cpp"
    },
    "catch2TestExplorer.executables": [{
        "name": "Single test",
        "pattern": "tests/compiled/*{test,Test,TEST}*",
        "cwd": "${absDirpath}",
        "env": {
            "C2TESTEXECUTABLEPATH": "${absPath}"
        }
    }],
    "catch2TestExplorer.defaultWatchTimeoutSec": 10
}